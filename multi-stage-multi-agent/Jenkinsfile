pipeline {
  agent none
  stages {
    stage('Back-end') {
      steps {
        sh 'docker pull maven'
      }
      agent {
        docker { image 'maven:latest' }
      }
      steps {
        sh 'mvn --version'
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'node --version'
      }
    }
  }
}
